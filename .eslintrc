{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended", "standard"],
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "rules": {
    "comma-dangle": ["error", "only-multiline"],
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
